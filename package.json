{
  "name": "@princethawani/ryuai",
  "version": "1.0.1",
  "description": "RyuAI â€“ Chatbot UI library built with Vue.js 2",
  "author": {
    "name": "Prince Thawani",
    "email": "princethawani4@gmail.com"
  },
  "license": "MIT",
  "main": "dist/vue-bot-ui.common.js",
  "module": "dist/vue-bot-ui.umd.js",
  "unpkg": "dist/vue-bot-ui.umd.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-bot-ui.umd.js",
      "require": "./dist/vue-bot-ui.common.js"
    }
  },
  "files": [
    "dist/vue-bot-ui.common.js",
    "dist/vue-bot-ui.umd.js",
    "dist/vue-bot-ui.umd.min.js",
    "dist/fonts",
    "dist/img",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "serve": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "build:lib": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --target lib --name vue-bot-ui ./src/vue-bot-ui.js",
    "clean:dist": "rm -rf dist && mkdir -p dist/fonts dist/img",
    "prep:lib": "npm run clean:dist && npm run build:lib && rm -f dist/*.1.js dist/demo.html && npm pack",
    "prepare": "npm run build:lib",
    "release": "standard-version && git push --follow-tags && npm publish --access public",
    "release:next": "standard-version --prerelease next && git push --follow-tags origin dev && npm publish --tag next",
    "lint": "vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit"
  },
  "keywords": [
    "chatbot",
    "vue",
    "bot-ui",
    "ryuai",
    "vue-bot-ui",
    "chat-interface",
    "vue2",
    "ui-library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/princethawani/ryuai.git"
  },
  "bugs": {
    "url": "https://github.com/princethawani/ryuai/issues"
  },
  "homepage": "https://github.com/princethawani/ryuai#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "emoji-picker-element": "^1.27.0",
    "emoji-picker-vue": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/jest": "^24.0.11",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^4.3.1",
    "core-js": "^3.3.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass": "1.32.13",
    "sass-loader": "^8.0.2",
    "standard-version": "^8.0.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.8.3",
    "vue-svg-loader": "^0.15.0",
    "vue-template-compiler": "^2.6.10"
  }
}
